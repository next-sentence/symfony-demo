{% set value = 'sylius.ui.' ~ data %}

{% if options.vars.labels is defined %}
    {% include [(options.vars.labels ~ ':' ~ data ~ '.html.twig'), ':Common/Label:_default.html.twig'] with {'value': value} %}
{% else %}
    {% include ':Common/Label:_default.html.twig' with {'value': value} %}
{% endif %}
