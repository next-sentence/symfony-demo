{% extends ':Frontend:layout.html.twig' %}

{% block title %} {{ page.metaTitle }} {% endblock %}
{% block meta_description %} {{ page.metaDescription }} {% endblock %}
{% block meta_keywords %} {{ page.metaKeywords }} {% endblock %}

{% block body %}
    <div id="wrapper">
        <div id="container">

            {{ page.title }}

            {% for block in page.blocks %}
                {{ block.content|raw }}
            {% endfor %}
        </div>
    </div>
{% endblock %}

